

@{
    ViewData["Title"] = "LoginForm";
}

<!DOCTYPE html>

<div>
   <div class="loginbox">
         <img class="loginPic" src="" alt="eye" /> <br />
         <br />
         <br />
         <hr />
         <h1>Login Here</h1>
         <form action="">
           <p>Username</p>
           <input type="text" name="username" placeholder="Enter Username" />
           <p>Password</p>
           <input type="password" name="" id="pass" placeholder="Enter Password" />
           <input type="submit" name="" id="submit" value="Login" /> <br />
           <hr />
           <div class="links">
              <a href="#">Login?</a>
           </div>
         </form>
       </div>   
</div>
