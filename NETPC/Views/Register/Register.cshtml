
@{
    ViewData["Title"] = "RegisterForm";
}

<!DOCTYPE html>


<div>
      <div class="registerbox">
            <img class="loginPic" src="Resources/engineer.png" alt="eye" /> <br />
            <br />
            <br />
            <hr />
            <h1>Login Here</h1>
            <form action="">
              <p>Username</p>
              <input type="text" name="username" placeholder="Enter Username" />
              <p>Password</p>
              <input type="password" name="" id="pass" placeholder="Enter Password" />
              <input type="submit" name="" id="submit" value="Login" /> <br />
              <hr />
              <div class="links">
                <a href="#">Forgot password?</a> <a href="#">SignUp?</a>
              </div>
            </form>
          </div>
</div>
